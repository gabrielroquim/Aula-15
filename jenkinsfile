pipeline {
    agent any
    
    stages {
       stage('Clonar o repositorio') {
            steps {
               git branch: 'master', url: 'https://github.com/gabrielroquim/Aula-15.git'
            }
        }
        
        stage('Instalar depÃªndencias') {
            steps {
               bat 'npm install'
            }
        }   
    
        stage('Executar os testes') {
            steps {
                bat'npm run cy:run'
            }        
        }
    
    }
}

